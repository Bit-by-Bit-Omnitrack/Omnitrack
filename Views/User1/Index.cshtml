@model IEnumerable<UserRoles.Models.Users>

<div class="card">
<div class="card-header">
    <h3 class="card-title">Omnitrack Users List</h3>
</div>
<div class="card-body">
    <table id="example1" class="table table-bordered table-striped">
    <thead>
        <tr>
                <th> @Html.DisplayNameFor(model => model.Email)</th>
                <th> @Html.DisplayNameFor(model => model.UserName)</th>
                    <th>
                        Status
                    </th>
                <th>
                    Edit
                </th>
                    <th>
                        View
                    </th>
            
                <th>
                    Delete
                </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
                <tr>
                    <td>@Html.DisplayFor(modelItem => user.Email)</td>
                    <td>@Html.DisplayFor(modelItem => user.UserName)</td>
                        <td>@Html.DisplayFor(modelItem => user.IsActive)</td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@user.Id" class="btn btn-primary">Edit</a>

                    </td>
                        <td>
                            <a asp-action="Details" asp-route-id="@user.Id" class="btn btn-warning">View</a>

                        </td>
                    <td>
                        <a asp-action="Delete" asp-route-id="@user.Id" class="btn btn-danger">Delete</a>

                    </td>
                </tr>
        }
       
    </tbody>
</table>
  </div>
  </div>